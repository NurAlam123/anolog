generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Post {
  id        String   @id @default(cuid()) @map("_id")
  title     String
  slug      String
  content   String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
